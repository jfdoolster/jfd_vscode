{
	"texeq": {
		"prefix": "texeq",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{equation}%",
		  "%",
		  "% equation",
		  "%",
		  "\\label{eq:${TM_FILENAME_BASE}}%",
		  "\\end{equation}%",
		],
		"description": "texeq"
	},
	"texfig": {
		"prefix": "texfig",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{figure}[ht]%",
		  "\\centering%",
		  "\\includegraphics[width=0.8\\textwidth]{path/to/image}",
		  "\\caption{\\captiontext}%}%",
		  "\\label{fig:$TM_FILENAME_BASE}%",
		  "\\end{figure}%",
		],
		"description": "texfig"
	},
	"texlst": {
		"prefix": "texlst",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{lstlisting}[%",
		  "\tlanguage=Python, % ",
		  "\tcaption={\\captiontext, % ",
		  "\tlabel={lst:$TM_FILENAME_BASE}, %",
		  "]",
		  "# Code...",
		  "\\end{lstlisting}%",
		],
		"description": "texlst"
	},
	"textab": {
		"prefix": "textab",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{table}[ht]%",
		  "\\centering%",
		  "\\begin{tabular}%",
		  "\\end{tabular}%",
		  "\\caption{\\captiontext}%",
		  "\\label{tab:$TM_FILENAME_BASE}%",
		  "\\end{table}%",
		],
		"description": "textab"
	},
	"Problem": {
	"prefix": "Problem",
	"body": [
		"\\Problem{}{}{%",
		"",
		"}"
	],
	"description": "Problem"
	}
}