{
	"texeq": {
		"prefix": "texeq",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{spacing}{1}",
		  "\\begin{gather}",
		  "%",
		  "% equation",
		  "%",
		  "\\label{${TM_FILENAME_BASE}}",
		  "\\end{gather}%",
		  "\\end{spacing}%"
		],
		"description": "texeq"
	},
	"texfig": {
		"prefix": "texfig",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{spacing}{1}",
		  "\\begin{figure}[ht]",
		  "\\centering",
		  "\\includegraphics[width=0.6\\textwidth]{%",
		  "%path_to_image",
		  "}",
		  "\\caption{%",
		  "${TM_FILENAME_BASE/[_]/\\_/g}",
		  "}\\label{$TM_FILENAME_BASE}%",
		  "\\end{figure}%",
		  "\\end{spacing}%"
		],
		"description": "texfig"
	},
	"texlst": {
		"prefix": "texlst",
		"body": [
		  "% ${RELATIVE_FILEPATH/[\\\\]/\\//g}",
		  "\\begin{spacing}{1}%",
		  "\\begin{lstlisting}[%",
		  "    language=Python, % ",
		  "    caption={${TM_FILENAME_BASE/[_]/\\\\_/g}}, % ",
		  "    label={$TM_FILENAME_BASE}, %",
		  "]",
