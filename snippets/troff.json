{
	// Place your snippets for troff here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
"manBase": {
  "prefix": "manBase",
  "body": [
    ".Dd $CURRENT_MONTH/$CURRENT_YEAR",
    ".Dt $TM_FILENAME_BASE(1) ",
    ".Os",
    ".Sh NAME ",
    ".Nm $TM_FILENAME_BASE",
    ".Nd Oneliner about sw",
    ".Sh SYNOPSIS",
    ".Nm",
    ".\\\" .Op Fl f Ar arg  \\\"",
    ".Op Fl hv",
    ".Pp",
    ".Sh DESCRIPTION",
    "More information on ",
    ".Nm",
    ".Pp \\\" spacing",
    ".Bl -tag -width indent-two",
    ".\\\" .It Op Fl f Ar arg ",
    ".\\\" Optional flag with argument",
    ".It Op Fl h ",
    "display usage information",
    ".It Op Fl v ",
    "display version",
    ".El",
    ".Pp",
    ".Sh SEE ALSO",
    ".\\\" .Xr manpage(#) ,",
    ".Pp"
  ],
  "description": "manBase"
}
}